<div class="container">
<p>Add a new movie in the list</p>

<%= simple_form_for [@list, @bookmark] do |f| %>
    <%= f.input :comment %>
    <%= f.association :movie %>
    <%= f.submit 'Submit', class: 'btn btn-danger mt-2' %>
  <% end %>

<%= link_to "back to movie list", list_path(@list) %>
</div>
